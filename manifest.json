{
    "name": "AtCoder Python unittest extension",
    "description": "Build an Extension!",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": [
            "*://*.contest.atcoder.jp/tasks/*",
            "*://atcoder.jp/contests/*/tasks/*"],
          "js": [
            "jquery-3.3.1.min.js",
            "content.js"]
        }
      ],
    "permissions" : [
        "clipboardWrite",
        "storage"
      ]
  }
  